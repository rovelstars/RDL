{
  "name": "RDL",
  "description": "Rovel Discord List",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run main && npm run sass",
    "main": "node src/index.js",
    "server": "node src/server/app.js",
    "postinstall": "node-sass --omit-source-map-url src/sass/default.scss src/public/assets/css/default.css"
  },
  "engines": {
    "node": "^15.x"
  },
  "dependencies": {
    "helmet": "4.4.1",
    "cookie-parser": "1.4.5",
    "disco-oauth": "5.0.1",
    "express-rate-limit": "5.2.6",
    "compression": "1.7.4",
    "mongodb": "3.6.4",
    "mongoose": "5.12.0",
    "express": "4.17.1",
    "ejs": "3.1.6",
    "rovel.js": "1.2.0",
    "discord.js": "12.5.1",
    "express-session": "1.17.1",
    "express-fileupload": "1.2.1",
    "passport": "0.4.1",
    "marked": "2.0.1",
    "@sentry/node": "6.1.0",
    "@sentry/tracing": "6.1.0",
    "module-alias": "2.2.2",
    "@meanie/mongoose-to-json": "2.5.0",
    "bulma": "0.9.2",
    "node-sass": "5.0.0"
  },
  "_moduleAliases": {
    "@root": "src",
    "@utils": "src/utils",
    "@routes": "src/server/routes",
    "@models": "src/models",
    "@views": "src/views",
    "@assets": "src/public/assets",
    "@bot": "src/bot",
    "@server": "src/server"
  }
}
